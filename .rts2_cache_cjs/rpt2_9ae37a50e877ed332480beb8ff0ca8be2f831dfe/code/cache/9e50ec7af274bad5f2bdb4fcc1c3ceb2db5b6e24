{"code":"import React, { createContext, useContext, useMemo } from 'react';\r\nimport invariant from 'tiny-invariant';\r\nimport { useWeb3ReactManager } from './manager';\r\nexport const PRIMARY_KEY = 'primary';\r\nconst CONTEXTS = {};\r\nexport function createWeb3ReactRoot(key) {\r\n    invariant(!CONTEXTS[key], `A root already exists for provided key ${key}`);\r\n    CONTEXTS[key] = createContext({\r\n        activate: async () => {\r\n            invariant(false, 'No <Web3ReactProvider ... /> found.');\r\n        },\r\n        setError: () => {\r\n            invariant(false, 'No <Web3ReactProvider ... /> found.');\r\n        },\r\n        deactivate: () => {\r\n            invariant(false, 'No <Web3ReactProvider ... /> found.');\r\n        },\r\n        active: false\r\n    });\r\n    CONTEXTS[key].displayName = `Web3ReactContext - ${key}`;\r\n    const Provider = CONTEXTS[key].Provider;\r\n    return function Web3ReactProvider({ getLibrary, children }) {\r\n        const { connector, provider, chainId, account, activate, setError, deactivate, error } = useWeb3ReactManager();\r\n        const active = connector !== undefined && chainId !== undefined && account !== undefined && !!!error;\r\n        const library = useMemo(() => active && chainId !== undefined && Number.isInteger(chainId) && !!connector\r\n            ? getLibrary(provider, connector)\r\n            : undefined, [active, getLibrary, provider, connector, chainId]);\r\n        const web3ReactContext = {\r\n            connector,\r\n            library,\r\n            chainId,\r\n            account,\r\n            activate,\r\n            setError,\r\n            deactivate,\r\n            active,\r\n            error\r\n        };\r\n        return React.createElement(Provider, { value: web3ReactContext }, children);\r\n    };\r\n}\r\nexport const Web3ReactProvider = createWeb3ReactRoot(PRIMARY_KEY);\r\nexport function getWeb3ReactContext(key = PRIMARY_KEY) {\r\n    invariant(Object.keys(CONTEXTS).includes(key), `Invalid key ${key}`);\r\n    return CONTEXTS[key];\r\n}\r\nexport function useWeb3React(key) {\r\n    return useContext(getWeb3ReactContext(key));\r\n}\r\n//# sourceMappingURL=provider.js.map","references":["/Backup/@Development/@BSC/@Project/@cheeseswap/@npm-modules/@connector/@develop/web3-react-core/node_modules/@types/react/index.d.ts","/Backup/@Development/@BSC/@Project/@cheeseswap/@npm-modules/@connector/@develop/web3-react-core/node_modules/tiny-invariant/dist/tiny-invariant.d.ts","/Backup/@Development/@BSC/@Project/@cheeseswap/@npm-modules/@connector/@develop/web3-react-core/src/types.ts","/Backup/@Development/@BSC/@Project/@cheeseswap/@npm-modules/@connector/@develop/web3-react-core/src/manager.ts"],"map":"{\"version\":3,\"file\":\"provider.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../src/provider.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAE,aAAa,EAAE,UAAU,EAAE,OAAO,EAAE,MAAM,OAAO,CAAA;AACjE,OAAO,SAAS,MAAM,gBAAgB,CAAA;AAGtC,OAAO,EAAE,mBAAmB,EAAE,MAAM,WAAW,CAAA;AAE/C,MAAM,CAAC,MAAM,WAAW,GAAG,SAAS,CAAA;AACpC,MAAM,QAAQ,GAAgE,EAAE,CAAA;AAOhF,MAAM,UAAU,mBAAmB,CAAC,GAAW;IAC7C,SAAS,CAAC,CAAC,QAAQ,CAAC,GAAG,CAAC,EAAE,0CAA0C,GAAG,EAAE,CAAC,CAAA;IAE1E,QAAQ,CAAC,GAAG,CAAC,GAAG,aAAa,CAA4B;QACvD,QAAQ,EAAE,KAAK,IAAI,EAAE;YACnB,SAAS,CAAC,KAAK,EAAE,qCAAqC,CAAC,CAAA;QACzD,CAAC;QACD,QAAQ,EAAE,GAAG,EAAE;YACb,SAAS,CAAC,KAAK,EAAE,qCAAqC,CAAC,CAAA;QACzD,CAAC;QACD,UAAU,EAAE,GAAG,EAAE;YACf,SAAS,CAAC,KAAK,EAAE,qCAAqC,CAAC,CAAA;QACzD,CAAC;QACD,MAAM,EAAE,KAAK;KACd,CAAC,CAAA;IACF,QAAQ,CAAC,GAAG,CAAC,CAAC,WAAW,GAAG,sBAAsB,GAAG,EAAE,CAAA;IAEvD,MAAM,QAAQ,GAAG,QAAQ,CAAC,GAAG,CAAC,CAAC,QAAQ,CAAA;IAEvC,OAAO,SAAS,iBAAiB,CAAC,EAAE,UAAU,EAAE,QAAQ,EAA8B;QACpF,MAAM,EACJ,SAAS,EACT,QAAQ,EACR,OAAO,EACP,OAAO,EAEP,QAAQ,EACR,QAAQ,EACR,UAAU,EAEV,KAAK,EACN,GAAG,mBAAmB,EAAE,CAAA;QAEzB,MAAM,MAAM,GAAG,SAAS,KAAK,SAAS,IAAI,OAAO,KAAK,SAAS,IAAI,OAAO,KAAK,SAAS,IAAI,CAAC,CAAC,CAAC,KAAK,CAAA;QACpG,MAAM,OAAO,GAAG,OAAO,CACrB,GAAG,EAAE,CACH,MAAM,IAAI,OAAO,KAAK,SAAS,IAAI,MAAM,CAAC,S